<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="{% block meta_description %}Bienvenue sur l'application web du Football Club de Rosheim situé dans la commune du même nom dans le Bas-Rhin{% endblock %}">
        <title>{% block title %}FC Rosheim{% endblock %}</title>
        <script src="https://kit.fontawesome.com/2e16462455.js" crossorigin="anonymous"></script>
        {% block stylesheets %}
            <link rel="shortcut icon" href="/img/logo.ico" type="image">

            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </head>
    <body class="w-100">

        {{ include ("include/_header.html.twig") }}
        <div class="col-1">
        {{ include ("include/_externalLinks.html.twig") }}
        </div>
        <div class="col-xxxl-11">
        {# <div class="row">
		<div class="col-2"></div>
		<div class="col-8">  #}
        {% block content %}
        {% endblock %}
        {# </div>
		<div class="col-2"></div>
        </div> #}
        </div>

    </body>
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
</html>
